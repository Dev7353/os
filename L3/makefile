CC=clang
CFLAGS=-Wall -pedantic -Werror -g

.PHONY: all clean

all: mthread work matrix forksqrt

clean: 
	$(RM) -rf *.o mthread work matrix forksqrt *.dSYM

mthread: mthread.c
	$(CC) $(CFLAGS) -pthread $< -o $@

work: work.c
	$(CC) $(CFLAGS) -lrt $< -o $@

matrix: matrix.c multiply_matrix.c

forksqrt: forksqrt.c ini.c
